1)a2b3c2 ---> aabbbcc


public class zoho {
    public static void main(String[] args) {
        String s="a2b3c2";
        char p=' ';
        for(char c: s.toCharArray()){

            if((c>='0' && c<='9')){
                int n=c-'0';
               for(int i=0;i<n;i++){
                   System.out.print(p);
               }
            }
            else {
                p=c;
            }
        }
    }
}



