public void preorder(Node root){

        if(root==null){
            return;
        }
        else {
            System.out.print(root.data+" ");
            preorder(root.left);
            preorder(root.right);
        }
    }
    public void postorder(Node root){

        Node temp=root;
        if(temp==null){
            return;
        }
        else {

            postorder(temp.left);
            postorder(temp.right);
            System.out.print(temp.data+" ");
        }
    }
    public void inorder(Node root){

        if(root==null){
            return;
        }
        else {

            inorder(root.left);
            System.out.print(root.data+" ");
            inorder(root.right);
        }
    }
