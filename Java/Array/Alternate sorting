public class New {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int i=0,j=0;
        int n=s.nextInt();
        int[] arr=new int[n];
        for(i=0;i<n;i++){
            arr[i]=s.nextInt();
        }
        i=0;
        j=n-1;

        while(i<j){
            System.out.print(" "+arr[j--]+" "+arr[i++]);
        }
        if (n%2!=0)
        System.out.println(arr[i]);



    }
}
